{
  "name": "@cocco3/website",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "init:git-hooks": "simple-git-hooks",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@tabler/icons-react": "^3.33.0",
    "clsx": "^2.1.1",
    "next": "^15.2.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.1.8",
    "nano-staged": "^0.8.0",
    "prettier": "^3.5.3",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5"
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged"
  },
  "nano-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{js,jsx,ts,tsx,json,css,md,mdx,html}": [
      "prettier --write"
    ]
  }
}
